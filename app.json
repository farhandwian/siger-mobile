{
  "expo": {
    "name": "siger-mobile",
    "slug": "siger-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sigermobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A365D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sigermobile.app",
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#1A365D",
        "tabletImage": "./assets/images/splash-icon.png"
      }
    },
    "android": {
      "package": "com.sigermobile.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDJ_su_d2pygaLLlbGmuP7A3ckGwSq-yF8"
        }
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "react-native-maps",
        {
          "iosGoogleMapsApiKey": "AIzaSyDJ_su_d2pygaLLlbGmuP7A3ckGwSq-yF8",
          "androidGoogleMapsApiKey": "AIzaSyDJ_su_d2pygaLLlbGmuP7A3ckGwSq-yF8"
        }
      ],
      "expo-router",
      "expo-location",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
